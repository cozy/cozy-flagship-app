{
  "name": "cozy-react-native",
  "version": "0.0.15",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build": "react-app-rewired build",
    "build:android:debug": "./scripts/build-debug-offline-apk.sh",
    "generate-boot-icon": "yarn react-native generate-bootsplash src/assets/icon-cozy.png --background-color=297EF2 --logo-width=160 --assets-path=src/assets --flavor=main",
    "home:build": "(cd cozy-home && yarn build:mobile)",
    "home:watch": "(cd cozy-home && yarn watch:mobile)",
    "install:home": "bash ./scripts/install-home.sh",
    "ios": "react-native run-ios",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "postinstall": "patch-package",
    "pre-commit": "yarn lint",
    "start": "react-native start",
    "test": "jest connectors/**/* src/**/*",
    "token": "node scripts/token.js",
    "web": "webpack serve --open --config webpack.config.js --inline --hot --port=9999",
    "web:test": "react-app-rewired test --env=jsdom"
  },
