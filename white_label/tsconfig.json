{
  "exclude": ["./**/*.spec.ts"],
  "extends": "cozy-tsconfig",
  "include":["./**/*"],
  "compilerOptions": {
    "allowJs": false,
    "emitDeclarationOnly": false,
    "lib": ["ES2021"],
    "module": "commonjs",
    "noEmit": false,
    "outDir": "./dist",
    "target": "ES2021",
    "paths": {
      "/*": ["../src/*"]
    },
  }
}
